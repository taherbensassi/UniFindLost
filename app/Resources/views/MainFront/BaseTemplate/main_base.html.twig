    <link rel="stylesheet" href="{{ asset('UserBundle/lib/ionicons/css/ionicons.min.css') }}">
    <link rel="stylesheet" href="{{ asset('UserBundle/lib/owlcarousel/assets/owl.carousel.min.css') }}">
    <link rel="stylesheet" href="{{ asset('UserBundle/lib/magnific-popup/magnific-popup.css') }}">
    <link rel="stylesheet" href="{{ asset('UserBundle/css/style.css') }}">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.4/css/select2.min.css" rel="stylesheet" />
    <link href="{{ asset('UserBundle/lib/select2-bootstrap.css') }}" rel="stylesheet" />
    {% endblock %}
    <style>
        #header-sticky-wrapper{
            height: 84.992px;
        }
    </style>

    {% block stylesheet %} {% endblock %}
</head>

<body id="body">
{% include 'MainFront/BaseTemplate/topbar.html.twig' %}
