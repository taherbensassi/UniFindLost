<!--==========================
          Intor Section
        ============================-->

<section id="intro">

    <section>
        <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div id="map_canvas" style="width: 100%;height: 401.82px"></div>
                </div>

            </div>

        </div>
    </section>
    <section class="search-sec">
        <div class="container">
            <form action="{{ path('searchForm') }}" method="get" >
                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <select class="form-control search-slt" id="categorselect" required>
                                    <option value="">Where did you Find/lost ?</option>
                                   {% for country in categorys %}
                                    <option value="{{ country.id }}" >{{ country.name }}</option>
                                    {% endfor %}

                                </select>
                            </div>

                            <div class="col-lg-6 col-md-6 col-sm-12 p-0">
                                <select class="form-control search-slt" required id="usersselect" name="usersselect">
                                    <option value="">Where ?</option>

                                </select>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-12 p-0">
                                <button type="submit" class="btn btn-danger wrn-btn">Search</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </section>

</section>

<!--==========================
          end Section
        ============================-->
