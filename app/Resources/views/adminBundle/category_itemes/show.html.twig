{% extends 'base.html.twig' %}

{% block body %}
    <h1>Category_iteme</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category_iteme.id }}</td>
            </tr>
            <tr>
                <th>Heading</th>
                <td>{{ category_iteme.heading }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if category_iteme.createdAt %}{{ category_iteme.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('category_itemes_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('category_itemes_edit', { 'id': category_iteme.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
